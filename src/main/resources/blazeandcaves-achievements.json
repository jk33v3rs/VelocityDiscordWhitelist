  "utility_functions": {
    "getXPForAchievement": {
      "description": "Calculate final XP value for an achievement",
      "parameters": ["achievement_id", "player_context", "bonus_modifiers"],
      "returns": "integer XP value"
    },
    "getRewardsForAchievement": {
      "description": "Get item and trophy rewards for an achievement",
      "parameters": ["achievement_id"],
      "returns": "reward object with items and trophies"
    },
    "isAchievementRateLimited": {
      "description": "Check if player is rate limited for achievement XP",
      "parameters": ["player_uuid", "current_time"],
      "returns": "boolean rate limit status"
    },
    "getCategoryProgress": {
      "description": "Get player progress in a specific category",
      "parameters": ["player_uuid", "category"],
      "returns": "progress object with completed achievements"
    }
  },
